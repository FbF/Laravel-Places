{
    "name": "fbf/laravel-places",
    "description": "A Laravel 4 package to add place listings to a site, e.g. bars, restaurants or hotels",
    "keywords": ["laravel","places"],
    "license": "MIT",
    "authors": [
        {
            "name": "Neil Crookes",
            "email": "neil.crookes@fivebyfiveuk.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "cviebrock/eloquent-sluggable": "1.0.*",
        "fzaninotto/faker": "1.3.*"
    },
    "autoload": {
        "classmap": [
            "src/controllers",
            "src/migrations",
            "src/models",
            "src/seeds"
        ],
        "psr-0": {
            "Fbf\\LaravelPlaces\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
